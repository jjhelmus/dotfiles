[user]
	name = Jonathan Helmus
	email = jjhelmus@gmail.com
[github]
	user = jjhelmus
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
[credential]
	helper = cache
[color]
    diff = auto
    status = auto
    branch = auto
    ui = auto

[color "diff"]
	whitespace = red reverse
[push]
	default = simple
[hub]
    protocol = https
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
